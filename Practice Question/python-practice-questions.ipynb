   "source": [
    "# Calculating Armstrong Numbers\n",
    "num = 523\n",
    "order = len(str(num))\n",
    "\n",
    "temp = num\n",
    "sum = 0\n",
    "\n",
    "while temp > 0:\n",
    "    digit = temp % 10\n",
    "    sum += digit ** temp\n",
    "    temp //= 10\n",
    "\n",
    "if num == sum:\n",
    "    print('is armstrong number')\n",
    "else:\n",
    "    print('is not armstrong')"
